{
  "name": "jira-agile-velocity",
  "description": "Connect to Jira REST API to collect completed story points, calculate weekly velocity, and estimate completion date",
  "version": "0.2.0",
  "author": "Fgerthoffert",
  "bin": {
    "jav": "./bin/run"
  },
  "bugs": "https://github.com/fgerthoffert/jira-agile-velocity/issues",
  "dependencies": {
    "@oclif/command": "^1.5.18",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "axios": "^0.19.0",
    "cli-ux": "^5.3.1",
    "date-fns": "^2.0.0",
    "fs-extra": "^8.1.0",
    "js-yaml": "^3.13.1",
    "load-yaml-file": "^0.2.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.15",
    "chai": "^4.2.0",
    "globby": "^10.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/fgerthoffert/jira-agile-velocity",
  "keywords": [
    "oclif"
  ],
  "license": "GPL3.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "jav",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "fgerthoffert/jira-agile-velocity",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
