{
  "version": 1,
  "projects": {
    "jav-ui": {
      "root": "apps/jav-ui",
      "sourceRoot": "apps/jav-ui/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/jav-ui",
            "index": "apps/jav-ui/src/index.html",
            "main": "apps/jav-ui/src/main.tsx",
            "polyfills": "apps/jav-ui/src/polyfills.ts",
            "tsConfig": "apps/jav-ui/tsconfig.app.json",
            "assets": ["apps/jav-ui/src/assets"],
            "styles": ["apps/jav-ui/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/babel"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/jav-ui/src/environments/environment.ts",
                  "with": "apps/jav-ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "jav-ui:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "jav-ui:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/jav-ui/.eslintrc",
            "tsConfig": [
              "apps/jav-ui/tsconfig.app.json",
              "apps/jav-ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/jav-ui/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/jav-ui/jest.config.js",
            "tsConfig": "apps/jav-ui/tsconfig.spec.json"
          }
        }
      }
    },
    "jav-ui-e2e": {
      "root": "apps/jav-ui-e2e",
      "sourceRoot": "apps/jav-ui-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/jav-ui-e2e/cypress.json",
            "tsConfig": "apps/jav-ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "jav-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "jav-ui:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/jav-ui-e2e/.eslintrc",
            "tsConfig": ["apps/jav-ui-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/jav-ui-e2e/**"]
          }
        }
      }
    },
    "jav-api": {
      "root": "apps/jav-api",
      "sourceRoot": "apps/jav-api/src",
      "projectType": "application",
      "prefix": "jav-api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/jav-api",
            "main": "apps/jav-api/src/main.ts",
            "tsConfig": "apps/jav-api/tsconfig.app.json",
            "assets": ["apps/jav-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/jav-api/src/environments/environment.ts",
                  "with": "apps/jav-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "jav-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/jav-api/.eslintrc",
            "tsConfig": [
              "apps/jav-api/tsconfig.app.json",
              "apps/jav-api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/jav-api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/jav-api/jest.config.js",
            "tsConfig": "apps/jav-api/tsconfig.spec.json"
          }
        }
      }
    },
    "jav-cli": {
      "root": "apps/jav-cli",
      "sourceRoot": "apps/jav-cli/src",
      "projectType": "application",
      "prefix": "jav-cli",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/jav-cli",
            "main": "apps/jav-cli/src/base.ts",
            "tsConfig": "apps/jav-cli/tsconfig.app.json",
            "assets": ["apps/jav-cli/src/assets"],
            "verbose": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/jav-cli/src/environments/environment.ts",
                  "with": "apps/jav-cli/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "jav-cli:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/jav-cli/.eslintrc",
            "tsConfig": [
              "apps/jav-cli/tsconfig.app.json",
              "apps/jav-cli/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/jav-cli/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/jav-cli/jest.config.js",
            "tsConfig": "apps/jav-cli/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "jav-ui"
}
